"language": "GDScript",
"path": "res://src/actions/action.gd"
}, {
"base": "Node2D",
"class": "ActionButton",
"language": "GDScript",
"path": "res://src/battle/ActionButton.gd"
}, {
"base": "Node2D",
"class": "Actions",
"language": "GDScript",
"path": "res://src/battle/Actions.gd"
}, {
"base": "Resource",
"class": "Actor",
"language": "GDScript",
"path": "res://src/player/actor.gd"
}, {
"base": "Node2D",
"class": "Battle",
"language": "GDScript",
"path": "res://src/core/Battle.gd"
}, {
"base": "Node2D",
"class": "Enemy",
"language": "GDScript",
"path": "res://src/core/EnemyUI.gd"
}, {
"base": "Position2D",
"class": "FloatingText",
"language": "GDScript",
"path": "res://assets/animations/FloatingText.gd"
}, {
"base": "Node",
